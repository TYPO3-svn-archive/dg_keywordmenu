<T3DataStructure>
<meta>
	<langDisable>1</langDisable>
</meta>
  <sheets>
	<sDEF>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_keywordmenu/locallang.xml:pi_flexform.sdef</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<what_to_display>
					<TCEforms>
					<label>LLL:EXT:dg_keywordmenu/locallang.xml:pi_flexform.what_to_display</label>
					<onChange>reload</onChange>
					<config>
						<type>select</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">KEYWORDS</numIndex>
								<numIndex index="1">KEYWORDS</numIndex>
							</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">MENU</numIndex>
									<numIndex index="1">MENU</numIndex>
							</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">BOTH</numIndex>
									<numIndex index="1">BOTH</numIndex>
							</numIndex>
						</items>
						<maxitems>1</maxitems>
						<size>1</size>
						<default>KEYWORDS</default>
					  </config>
					  </TCEforms>
			    </what_to_display>
			    <template_file>
					<TCEforms>
						<label>LLL:EXT:dg_keywordmenu/locallang.xml:pi_flexform.template_file</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
							<wizards type="array">
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>tmpl,tmp,html,htm</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
					</TCEforms>
				</template_file>
				<single_pid>
					<TCEforms>
					<label>LLL:EXT:dg_keywordmenu/locallang.xml:pi_flexform.single_pid</label>
					<displayCond>FIELD:what_to_display:=:MENU</displayCond>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</single_pid>
				<storage_pid>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
						<displayCond>FIELD:what_to_display:=:KEYWORDS,BOTH</displayCond>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</storage_pid>
			</el>
		</ROOT>
	</sDEF>
	<sKEY>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_keywordmenu/locallang.xml:pi_flexform.skey</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<keywords>
					<TCEforms>
					 	<exclude>1</exclude>
						<label>LLL:EXT:dg_keywordmenu/locallang.xml:pi_flexform.keywords</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>tx_dgkeywordmenu_keywords</allowed>
							<size>6</size>
							<maxitems>1000</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</keywords>
			</el>
		</ROOT>
	</sKEY>
</sheets>
</T3DataStructure>